{
  "name": "server",
  "version": "0.0.0",
  "private": true,
  "author": {
    "name": "Ben Chavez",
    "email": "ben.m.chavez@gmail.com",
    "url": "https://benjamin-chavez.com/"
  },
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf dist",
    "build:clean": "pnpm run clean && pnpm run build",
    "dev": "nodemon --exec \"node -r esbuild-register ./src/server.ts\" -e .ts",
    "dev:clean": "(dropdb audio_archive_development || true) && createdb audio_archive_development && knex migrate:latest && knex seed:run && NODE_ENV=development pnpm dev",
    "lint": "tsc --noEmit && eslint \"src/**/*.ts*\"",
    "start": "node -r esbuild-register ./src/server.ts",
    "test": "jest --detectOpenHandles",
    "db:migrate": "knex migrate:latest && node ./update-types",
    "db:reset:prod": "npm run db:drop:prod && npm run db:create:prod && pnpm run db:migrate:prod && pnpm run db:seed:prod",
    "db:drop:prod": "dropdb -h localhost -p 5432 -U postgres audio_archive_production || true",
    "db:create:prod": "createdb -h localhost -p 5432 -U postgres audio_archive_production || true",
    "db:migrate:prod": "NODE_ENV=production knex migrate:latest",
    "db:seed:prod": "NODE_ENV=production knex seed:run"
  },
  "jest": {
    "preset": "jest-presets/jest/node"
  },
  "dependencies": {
    "body-parser": "^1.20.2",
    "@aws-sdk/client-s3": "^3.427.0",
    "@aws-sdk/s3-request-presigner": "^3.427.0",
    "amqplib": "^0.10.3",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-async-handler": "^1.2.0",
    "express-list-endpoints": "^6.0.0",
    "knex": "^3.0.1",
    "zod": "^3.22.2",
    "logger": "workspace:*",
    "@shared": "workspace:*",
    "morgan": "^1.10.0",
    "multer": "1.4.5-lts.1",
    "stripe": "^14.3.0",
    "debug": "~4.3.4",
    "cookie-parser": "~1.4.6",
    "kysely-codegen": "^0.10.1",
    "pg-to-ts": "^4.1.1",
    "connect-session-knex": "^3.0.1",
    "express-flash": "^0.0.2",
    "express-jwt": "^8.4.1",
    "express-oauth2-jwt-bearer": "^1.5.0",
    "express-openid-connect": "^2.16.0",
    "express-session": "^1.17.3",
    "helmet": "^7.0.0",
    "jwks-rsa": "^3.0.1",
    "pg": "^8.11.3"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.5",
    "@types/cors": "^2.8.16",
    "@types/express": "^4.17.21",
    "@types/express-list-endpoints": "^6.0.3",
    "@types/jest": "^29.5.8",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.9.2",
    "@types/supertest": "^2.0.16",
    "esbuild": "^0.19.5",
    "esbuild-register": "^3.5.0",
    "eslint": "^8.54.0",
    "eslint-config-custom-server": "workspace:*",
    "jest": "^29.7.0",
    "jest-presets": "workspace:*",
    "knex-types": "^0.5.0",
    "nodemon": "^3.0.1",
    "supertest": "^6.3.3",
    "@types/multer": "^1.4.8",
    "concurrently": "^8.2.2",
    "@types/amqplib": "^0.10.3",
    "tsconfig": "workspace:*",
    "typescript": "^5.2.2",
    "@types/express-flash": "^0.0.2",
    "@aws-sdk/types": "^3.425.0",
    "@types/cookie-parser": "^1.4.3",
    "@types/express-session": "^1.17.7",
    "@types/pg": "^8.10.2",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1"
  }
}
